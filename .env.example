# ===========================================
# EHR Data Explainer - Environment Config
# ===========================================
# Copy this file to .env and fill in your values

# Neo4j Database
# For local: bolt://localhost:7687
# For AuraDB: neo4j+s://xxxxx.databases.neo4j.io
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# Anthropic Claude API
# Get your key at: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-your-key-here

# Claude Model Selection
# Options: claude-opus-4-20250514 (best), claude-sonnet-4-20250514 (faster)
CLAUDE_MODEL=claude-sonnet-4-20250514

# Hugging Face Token (for Wan 2.2 video generation)
# Get your token at: https://huggingface.co/settings/tokens
HF_TOKEN=hf_your-token-here

# Debug Mode
DEBUG=true
